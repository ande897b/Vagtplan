<Window x:Class="Vagtplan.CreateRosterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vagtplan"
        mc:Ignorable="d"
        Title="CreateRosterWindow" Height="450" Width="800">
    <Grid>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="243,175,0,0" VerticalAlignment="Top" Width="120" SelectedIndex="1">
            <ComboBoxItem x:Name="comboBox1Month" Content="1 Måned" HorizontalAlignment="Left" Width="118"/>
            <ComboBoxItem x:Name="comboBox2Months" Content="2 Måneder" HorizontalAlignment="Left" Width="118"/>
            <ComboBoxItem x:Name="comboBox3Months" Content="3 Måneder" HorizontalAlignment="Left" Width="118"/>
        </ComboBox>
        <Label x:Name="label" Content="Vælg Varighed af Vagtplan" HorizontalAlignment="Left" Margin="223,134,0,0" VerticalAlignment="Top" Height="36" Width="172"/>
        <Button x:Name="button" Content="Opret" HorizontalAlignment="Left" Margin="383,175,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
