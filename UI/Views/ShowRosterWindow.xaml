        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UI.Views"
        mc:Ignorable="d"
        Title="ShowRosterWindow" Height="450" Width="925">
    <Grid Margin="0,0,2,0">
        <TextBox x:Name="weekday1txtbox" HorizontalAlignment="Left" Height="236" Margin="20,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" IsEnabled="False"/>
