        <TextBox x:Name="weekday4txtbox" HorizontalAlignment="Left" Height="236" Margin="412,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" IsEnabled="False"/>
        <TextBox x:Name="weekday5txtbox" HorizontalAlignment="Left" Height="236" Margin="538,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" IsEnabled="False"/>
        <TextBox x:Name="weekday6txtbox" HorizontalAlignment="Left" Height="236" Margin="661,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" IsEnabled="False"/>
        <TextBox x:Name="weekday7txtbox" HorizontalAlignment="Left" Height="236" Margin="785,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100"/>
        <Label x:Name="weekday1label" Content="Label" HorizontalAlignment="Left" Margin="20,10,0,0" VerticalAlignment="Top" Width="100"/>
        <Label x:Name="weekday2label" Content="Label" HorizontalAlignment="Left" Margin="149,10,0,0" VerticalAlignment="Top" Width="100"/>
        <Label x:Name="weekday3label" Content="Label" HorizontalAlignment="Left" Margin="277,14,0,0" VerticalAlignment="Top" Width="100"/>
        <Label x:Name="weekday4label" Content="Label" HorizontalAlignment="Left" Margin="412,14,0,0" VerticalAlignment="Top" Width="100"/>
        <Label x:Name="weekday5label" Content="Label" HorizontalAlignment="Left" Margin="538,14,0,0" VerticalAlignment="Top" Width="100"/>
        <Label x:Name="weekday6label" Content="Label" HorizontalAlignment="Left" Margin="661,14,0,0" VerticalAlignment="Top" Width="100"/>
        <Label x:Name="weekday7label" Content="Label" HorizontalAlignment="Left" Margin="785,14,0,0" VerticalAlignment="Top" Width="100"/>
        <Button x:Name="saveBtn" Content="Gem Vagter" HorizontalAlignment="Left" Margin="811,370,0,0" VerticalAlignment="Top" Width="94" Height="39"/>
        <Button x:Name="nextWeekBtn" Content="Næste Uge" HorizontalAlignment="Left" Margin="811,326,0,0" VerticalAlignment="Top" Width="94" Height="39"/>

    </Grid>
</Window>
